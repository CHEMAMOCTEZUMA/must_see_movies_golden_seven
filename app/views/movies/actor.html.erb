<h1>List of Actors</h1>

<a href="http://localhost:3000/photos/new">Add a New Movie</a>

<table>
  <tr>
    <th>Actor</th>
    <th>Name</th>
  </tr>
  <% @list_of_actors.each do |actor| %>
  <tr>
    <td>
      <img src="<%= actor.image_url %>" width="80">
    </td>
    <td>
      <%= actor.name %>
    </td>
     <td>
      <a href="http://localhost:3000/actors/<%= actor.id %>">Details</a>
    </td>
    </tr>
  <% end %>
</table>